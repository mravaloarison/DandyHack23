{
  "manifest_version": 3,
  "name": "Magic Mouse 🐁",
  "version": "1.0",
  "description": "An extension that explains highlighted text. 🪄",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"]
    }
  ]
}
  